{
	"name": "lonicera.ca",
	"version": "9.0.0",
	"description": "lonicera.ca website using Eleventy",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/lonicera.ca/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/lonicera.ca/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/plansmash/lonicera.ca.git"
	},
	"author": {
		"name": "Shawn Thompson",
		"url": "https://shawnthompson.ca/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/plansmash/lonicera.ca/issues"
	},
	"homepage": "https://github.com/plansmash/lonicera.ca#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"cross-env": "^7.0.3",
		"luxon": "^3.5.0"
	},
	"dependencies": {
	}
}
